{
 "trailingComma": "es5",
 "tabWidth": 2,
 "singleQuote": false,
 "semi": true,
 "printWidth": 80,
 "useTabs": false,
 "bracketSpacing": true,
 "arrowParens": "always",
 "parser": "typescript",
 "endOfLine": "auto",
 "importOrder": [
  "<THIRD_PARTY_MODULES>",
  "^@/components/(.*)$",
  "^@/layout/(.*)$",
  "^@/ui/(.*)$",
  "^@/providers/(.*)$",
  "^@/shared/(.*)$",
  "^@/assets/(.*)$",
  "^@/config/(.*)$",
  "^@/store/(.*)$",
  "^@/hooks/(.*)$",
  "^@/types/(.*)$",
  "^@/utils/(.*)$",
  "^@/api/(.*)$",
  "^@../(.*)$",
  "^@./(.*)$"
 ],
 "importOrderSeparation": true,
 "importOrderSortSpecifier": true
}